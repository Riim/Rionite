  "bugs": {
    "url": "https://github.com/Riim/Rionite/issues"
  },
  "homepage": "https://github.com/Riim/Rionite#readme",
  "devDependencies": {
    "@riim/defer": "^1.1.2",
    "@riim/escape-html": "^1.1.5",
    "@riim/gettext": "^2.0.0",
    "@riim/kebab-case": "^1.0.0",
    "@riim/move-content": "^1.0.0",
    "@riim/next-tick": "^1.2.6",
    "@riim/pascalize": "^1.0.1",
